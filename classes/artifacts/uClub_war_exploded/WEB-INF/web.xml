<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClubInfo</servlet-name>
        <servlet-class>servlet.ClobInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClubInfo</servlet-name>
        <url-pattern>/ClubInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClubInfoChange</servlet-name>
        <servlet-class>servlet.ClubInfoChange</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClubInfoChange</servlet-name>
        <url-pattern>/ClubInfoChange</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ActCreate</servlet-name>
        <servlet-class>servlet.ActCreate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActCreate</servlet-name>
        <url-pattern>/ActCreate</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ActCreateApply</servlet-name>
        <servlet-class>servlet.ActCreateApply</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActCreateApply</servlet-name>
        <url-pattern>/ActCreateApply</url-pattern>
    </servlet-mapping>


</web-app>