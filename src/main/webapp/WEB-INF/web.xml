<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>clubDetail</servlet-name>
        <servlet-class>controller.ClubDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clubDetail</servlet-name>
        <url-pattern>/clubDetail</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>activeDetail</servlet-name>
        <servlet-class>controller.ActiveDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>activeDetail</servlet-name>
        <url-pattern>/activeDetail</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>joinClubApply</servlet-name>
        <servlet-class>controller.JoinClubApply</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>joinClubApply</servlet-name>
        <url-pattern>/joinClubApply</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>joinApply</servlet-name>
        <servlet-class>controller.JoinApply</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>joinApply</servlet-name>
        <url-pattern>/joinApply</url-pattern>
    </servlet-mapping>


</web-app>